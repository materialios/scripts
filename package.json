{
  "name": "@materialios/scripts",
  "version": "0.1.9",
  "description": "@materialios's scripts",
  "private": false,
  "main": "lib/index.js",
  "bin": {
    "materialios": "bin/materialios.js"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && npx babel src --out-dir lib && npx include-css --origin=./src/initIndexAndApp/templates --destination=./lib/initIndexAndApp/templates && npm --no-git-tag-version version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/materialios/scripts.git"
  },
  "keywords": [
    "@materialios/scripts",
    "materialios",
    "scripts",
    "material",
    "design",
    "android",
    "ios",
    "ui",
    "ux"
  ],
  "author": "Muhammad Rendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/materialios/scripts/issues"
  },
  "homepage": "https://github.com/materialios/scripts#readme",
  "babel": {
    "presets": [
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-react": "^7.12.13",
    "@mnrendra/include-css": "^0.1.6"
  },
  "dependencies": {
    "@mnrendra/lssync": "^0.1.4"
  }
